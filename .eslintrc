{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "rules": {
    "arrow-parens": ["error", "as-needed"],
    "comma-dangle": 0,
    "consistent-return": [2, { "treatUndefinedAsUnspecified": true }],
    "max-len": [2, 200, 2, {
      "ignoreUrls": true,
      "ignoreComments": false
    }]
  }
}